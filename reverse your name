// reverse any string using char array method


public class main{
    public static void main(String[] args){
        String name = "Suraj";
        int n = name.length();
        char[] c = name.toCharArray();
        String s = "";
        for(int i=n-1;i>=0;i--){
            s+=c[i];
            
        }
        System.out.print(s);
    }
}
